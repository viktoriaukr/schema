{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "title": "Root Schema",
  "type": "object",
  "default": {},
  "required": [
    "isbn",
    "amazon_url",
    "author",
    "language",
    "pages",
    "publisher",
    "title",
    "year"
  ],
  "properties": {
    "isbn": {
      "title": "The isbn Schema",
      "type": "string",
      "default": "",
      "examples": ["0691161518"]
    },
    "amazon_url": {
      "title": "The amazon_url Schema",
      "type": "string",
      "default": "",
      "examples": ["http://a.co/eobPtX2"]
    },
    "author": {
      "title": "The author Schema",
      "type": "string",
      "default": "",
      "examples": ["Matthew Lane"]
    },
    "language": {
      "title": "The language Schema",
      "type": "string",
      "default": "",
      "examples": ["english"]
    },
    "pages": {
      "title": "The pages Schema",
      "type": "number",
      "default": 0,
      "examples": [264]
    },
    "publisher": {
      "title": "The publisher Schema",
      "type": "string",
      "default": "",
      "examples": ["Princeton University Press"]
    },
    "title": {
      "title": "The title Schema",
      "type": "string",
      "default": "",
      "examples": ["Power-Up: Unlocking the Hidden Mathematics in Video Games"]
    },
    "year": {
      "title": "The year Schema",
      "type": "number",
      "default": 0,
      "examples": [2017]
    }
  },
  "examples": [
    {
      "isbn": "0691161518",
      "amazon_url": "http://a.co/eobPtX2",
      "author": "Matthew Lane",
      "language": "english",
      "pages": 264,
      "publisher": "Princeton University Press",
      "title": "Power-Up: Unlocking the Hidden Mathematics in Video Games",
      "year": 2017
    }
  ]
}
